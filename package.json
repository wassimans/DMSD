{
  "name": "dmsd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "anvil": "source .env && anvil --fork-url $MUMBAI_RPC_URL --chain-id $MUMBAI_CHAIN_ID",
    "deploy": "source .env && forge create contracts/src/DMSD.sol:DMSD --rpc-url $FORGE_RPC_URL --private-key $FORGE_PRIVATE_KEY --etherscan-api-key $ETHERSCAN_API_KEY --verify",
    "deploy:goerli": "source .env && forge create contracts/src/DMSD.sol:DMSD --rpc-url $MUMBAI_RPC_URL --private-key $PRIVATE_KEY --etherscan-api-key $ETHERSCAN_API_KEY --verify",
    "deploy:anvil": "source .env && forge create contracts/src/DMSD.sol:DMSD --rpc-url $FORGE_RPC_URL --private-key $FORGE_PRIVATE_KEY --legacy",
    "dev": "npm run wagmi && next dev",
    "dev:foundry": "npm run dev & wagmi generate --watch & npm run anvil",
    "build:wagmi:next": "npm run wagmi && next build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "wagmi": "wagmi generate"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@moralisweb3/next": "^2.18.2",
    "@rainbow-me/rainbowkit": "^0.11.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@wagmi/cli": "^0.1.10",
    "axios": "^1.3.5",
    "buffer": "^6.0.3",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "ethers": "^5.7.2",
    "framer-motion": "^10.10.0",
    "moralis": "^2.18.2",
    "next": "13.3.0",
    "next-auth": "^4.21.1",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "typescript": "5.0.4",
    "util": "^0.12.4",
    "wagmi": "^0.11.7"
  }
}
